server:
  # Sets the custom port
  port: 443
  address: 0.0.0.0

lavalink:
  server:
    # CRITICAL FIX: Password must match the client config exactly
    password: "bloodyclutch123"

    sources:
      youtube: false # Disabled because you use the plugin
      bandcamp: true
      soundcloud: true
      twitch: true
      vimeo: true
      http: true
      local: false
      
    bufferDurationMs: 400
    frameBufferDurationMs: 5000
    youtubePlaylistLoadLimit: 6
    playerUpdateInterval: 5
    youtubeSearchEnabled: true
    soundcloudSearchEnabled: true
    gc-warnings: true
  
  plugins:
    # YouTube Plugin dependency
    - dependency: "dev.lavalink.youtube:youtube-plugin:1.15.0"
      repository: "https://maven.lavalink.dev/releases"
