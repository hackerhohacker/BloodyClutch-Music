server:
  port: 443
  address: 0.0.0.0
  ssl:
    enabled: false

lavalink:
  server:
    password: "bloodyclutch123"
    sources:
      youtube: true
      bandcamp: true
      soundcloud: true
      twitch: true
      vimeo: true
      mixer: true
      youtubeMusic: true
      spotify: true

  # The filter timeout in seconds.
  # The default is 5 seconds, which is too low for some large tracks.
  # Setting it to 10 seconds is a safe value.
  filter-timeout: 10

logging:
  level:
    root: INFO
    lavalink: INFO

plugins:
  - dependency: com.github.topi314.lavasrc:lavasrc-plugin:4.8.1
    repository: https://maven.topi.moe/releases
