server:
  port: 443   # FIX: Forces Lavalink to use port 443
  address: 0.0.0.0
  ssl:
    enabled: false

lavalink:
  server:
    password: "bloodyclutch123" # Must match bot config
    sources:
      youtube: true
      bandcamp: true
      soundcloud: true
      twitch: true
      vimeo: true
      youtubeMusic: true
      spotify: true

  # The filter timeout in seconds.
  filter-timeout: 10

logging:
  level:
    root: INFO
    lavalink: INFO

plugins:
  - dependency: com.github.topi314.lavasrc:lavasrc-plugin:4.8.1
    repository: https://maven.topi.moe/releases
